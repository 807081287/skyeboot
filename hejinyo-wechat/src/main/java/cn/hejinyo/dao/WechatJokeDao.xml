<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hejinyo.dao.WechatJokeDao">
    <resultMap id="BaseResultMap" type="cn.hejinyo.model.WechatJoke">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="title" jdbcType="VARCHAR" property="title"/>
        <result column="typeid" jdbcType="INTEGER" property="typeid"/>
        <result column="hits" jdbcType="INTEGER" property="hits"/>
        <result column="dateandtime" jdbcType="TIMESTAMP" property="dateandtime"/>
        <result column="content" jdbcType="LONGVARCHAR" property="content"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, title, typeid, hits, dateandtime
    </sql>
    <select id="get" parameterType="int" resultMap="BaseResultMap">
        SELECT
            id,
            title,
            typeid,
            hits,
            dateandtime,
            content
        FROM wechat_joke
        WHERE id = #{id}
    </select>
</mapper>